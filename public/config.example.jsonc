{
	// Token configuration
	"token": {
		"iss": "https://webitel.example.com/portal",
		"endpointUrl": "https://webitel.example.com/api/portal/token",
		"appToken": "<token>"
	},

	// Call configuration
	"call": {
		"host": "wss://webitel.example.com/wssip/",
		"target": "service",
		"videoDeviceResolution": {
			"width": {
				"ideal": 1920 // note! only "ideal" is supported
			},
			"height": {
				"ideal": 1080 // note! only "ideal" is supported
			}
		}
	},

	// Assets configuration
	"assets": {
		"logoPicture": "/meet/assets/web-logo.svg",
		"mainBackground": "/meet/assets/web-background.svg"
	},

	// Evaluation configuration
	"evaluation": {
		"goodGrade": 10,
		"badGrade": 5
	},

	// Language configuration
	"lang": "en" // ['en', 'ru', 'uk']
}
